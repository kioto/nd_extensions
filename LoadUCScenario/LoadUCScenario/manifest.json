{
	"$schema": "manifest.schema.json",

	"name": "LoadUCScenario", // TODO ここはすべてのエクステンションでユニークになるようにして下さい。
	"main": "LoadUCScenario.dll", // TODO ここはプロジェクトのアセンブリファイル名と同じにしてください。
	
	"lifecycle": "application",

	"extensionPoints": {

		"commands": [
			{
				"id": "LoadUCScenario.LoadCommand",
				"execFunc": "Load"
			}
		],

		"events": {
		},

		"ribbon": {
			"tabs": [
				{
					"id": "LoadUCScenario.LoadTab",
					"label": "UCS Load",

					"groups": [
						{
							"id": "LoadUCScenario.Group1",
							"label": "読み込み",

							"controls": [
								{
									"id": "LoadUCScenario.LoadButton",
									"type": "Button",
									"label": "Load",
									"imageLarge": "resources/images/excel-icon.png",
									"command": "LoadUCScenario.LoadCommand"
								}
							]
						}
					]
				}
			]
		}


	}

}
